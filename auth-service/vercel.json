{
  "version": 2,
  "builds": [
    {
      "src": "api/auth.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth/[...all].ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/[...all].ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/auth.ts"
    }
  ]
}