{
  "version": 2,
  "builds": [
    {
      "src": "backend/app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/app/main.py"
    },
    {
      "src": "/health",
      "dest": "backend/app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "backend/app/main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}